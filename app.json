{
  "expo": {
    "name": "Rapid Storage",
    "slug": "rapid-storage",
    "version": "1.0.1",
    "scheme": "com.rapidstorage",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [
      "expo-dev-client",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Rapid Storage needs access to your photo library to let you select and upload images and videos."
        }
      ]
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rapidstorage.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.rapidstorage.app",
      "versionCode": 2
    },
    "extra": {
      "eas": {
        "projectId": "b1773edf-ad8f-4a0b-aae7-a4c588fa7b0e"
      }
    },
    "updates": {
      "url": "https://u.expo.dev/b1773edf-ad8f-4a0b-aae7-a4c588fa7b0e",
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "aryankumar877"
  }
}
